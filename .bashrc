alias fzf="fzf --color='bg:#002b36,bg+:#073642,info:#839496,border:#586e75,spinner:#93a1a1' --color='hl:#268bd2,fg:#839496,header:#586e75,fg+:#fdf6e3' --color='pointer:#268bd2,marker:#2aa198,prompt:#268bd2,hl+:#fdf6e3',gutter:-1"

shopt -s extglob

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
